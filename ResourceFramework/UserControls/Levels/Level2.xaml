<UserControl x:Class="ResourceFramework.UserControls.Levels.Level2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">
    
    <Grid x:Name="LayoutRoot">
		<!-- Level Surface ME -->
		<Canvas x:Name="levelSurfaceME" Canvas.ZIndex="2" />

		<!-- Level Surface Kraker -->
		<Canvas x:Name="levelSurfaceKraker" Canvas.ZIndex="1" />

		<!-- Level Background -->
		<Canvas x:Name="LevelBackground" Width="1600" Height="1200" Canvas.Left="0" Canvas.Top="0" Canvas.ZIndex="0">
			<TextBlock Margin="128,80,136,116" TextWrapping="Wrap" Text="2" Foreground="#FF0056FF" FontSize="66.667" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
		</Canvas>
    </Grid>
</UserControl>
